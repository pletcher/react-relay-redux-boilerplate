    "url": "https://github.com/pletcher/mission-hacks/issues"
  },
  "homepage": "https://github.com/pletcher/mission-hacks",
  "dependencies": {
    "autoprefixer": "^6.2.3",
    "basscss": "^7.1.0",
    "bcrypt": "^0.8.5",
    "classnames": "^2.2.3",
    "cookie-parser": "^1.4.1",
